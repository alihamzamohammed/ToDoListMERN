{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": "src/__tests__"
    },
    "templates": {
        "better-docs": {
            "name": "To Do List MERN"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.MD"
    }
}